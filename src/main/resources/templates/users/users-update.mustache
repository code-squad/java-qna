{{>layout/header}}
<h1>회원정보 수정</h1>
<div class="col-md-12">
  <div class="col-md-4">
    <form>
      <div class="form-group">
        <label for="title">회원 고유번호</label>
        <input type="text" class="form-control" id="Id" value="{{user.Id}}" readonly>
      </div>
      <div class="form-group">
        <label for="title">회원 아이디</label>
        <input type="text" class="form-control" id="userId" value="{{user.userId}}">
      </div>
      <div class="form-group">
        <label for="title">회원 비밀번호</label>
        <input type="text" class="form-control" id="password" value="{{user.password}}">
      </div>
      <div class="form-group">
        <label for="title">이름</label>
        <input type="text" class="form-control" id="name" value="{{user.name}}">
      </div>
      <div class="form-group">
        <label for="title">이메일</label>
        <input type="text" class="form-control" id="email" value="{{user.email}}">
      </div>
    </form>
    <a href="/" role="button" class="btn btn-secondary">취소</a>
    <button type="button" class="btn btn-primary" id="btn-userupdate">수정 완료</button>
    <button type="button" class="btn btn-danger" id="btn-delete">삭제</button>
  </div>
</div>
{{>layout/footer}}